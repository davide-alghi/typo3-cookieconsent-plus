				<source>any value</source>
			</trans-unit>
			<trans-unit id="ttcontent.tx_cookieconsentplus_conditionvalue.denied">
				<source>denied</source>
			</trans-unit>
			<trans-unit id="ttcontent.tx_cookieconsentplus_conditionvalue.accepted">
